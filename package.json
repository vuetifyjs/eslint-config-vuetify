{
  "name": "eslint-config-vuetify",
  "version": "4.3.4",
  "description": "eslint config for vue.js projects",
  "type": "module",
  "bin": {
    "eslint-config-vuetify": "bin/cli.mjs"
  },
  "scripts": {
    "typegen": "tsx scripts/typegen.ts",
    "build": "node --run typegen && unbuild",
    "prepack": "node --run build",
    "stub": "unbuild --stub",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test": "vitest run",
    "dev": "vitest",
    "dev:prepare": "node --run typegen && node --run stub",
    "release": "bumpp -r"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuetifyjs/eslint-config-vuetify.git"
  },
  "keywords": [
    "eslint",
    "config",
    "eslint-config",
    "vue",
    "vuetify"
  ],
  "author": "John Leider <john@vuetifyjs.com>",
  "license": "MIT",
  "peerDependencies": {
    "@vitest/eslint-plugin": "^1.1.42",
    "eslint": "^9.5.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-vuejs-accessibility": "^2.0.0"
  },
  "peerDependenciesMeta": {
    "eslint-plugin-import-x": {
      "optional": true
    },
    "eslint-plugin-jest": {
      "optional": true
    },
    "eslint-plugin-no-only-tests": {
      "optional": true
    },
    "@vitest/eslint-plugin": {
      "optional": true
    },
    "eslint-plugin-vuejs-accessibility": {
      "optional": true
    }
  },
  "dependencies": {
    "@clack/prompts": "^0.11.0",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@stylistic/eslint-plugin": "^5.6.1",
    "@typescript-eslint/parser": "^8.50.0",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-flat-config-utils": "^2.1.4",
    "eslint-plugin-antfu": "^3.1.1",
    "eslint-plugin-import-lite": "^0.3.1",
    "eslint-plugin-jsonc": "^2.21.0",
    "eslint-plugin-perfectionist": "^4.15.1",
    "eslint-plugin-pnpm": "^1.4.3",
    "eslint-plugin-regexp": "^2.10.0",
    "eslint-plugin-unicorn": "^62.0.0",
    "eslint-plugin-vue": "^10.6.2",
    "eslint-typegen": "^2.3.0",
    "exsolve": "^1.0.8",
    "globals": "^16.5.0",
    "jsonc-eslint-parser": "^2.4.2",
    "kolorist": "^1.8.0",
    "local-pkg": "^1.1.2",
    "nypm": "^0.6.2",
    "package-manager-detector": "^1.6.0",
    "tinyexec": "^1.0.2",
    "typescript-eslint": "^8.50.0",
    "vue-eslint-parser": "^10.2.0",
    "yaml-eslint-parser": "^1.3.2"
  },
  "devDependencies": {
    "@types/node": "^22.19.3",
    "bumpp": "^10.3.2",
    "changelogithub": "^14.0.0",
    "eslint": "^9.39.2",
    "eslint-config-vuetify": "file:",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "unbuild": "^3.6.1",
    "valibot": "^1.2.0",
    "vitest": "^4.0.16",
    "vue": "^3.5.26"
  },
  "packageManager": "pnpm@10.26.1"
}
