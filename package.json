{
  "name": "eslint-config-vuetify",
  "version": "4.0.0",
  "description": "eslint config for vue.js projects",
  "type": "module",
  "scripts": {
    "typegen": "tsx scripts/typegen.ts",
    "build": "node --run typegen && unbuild",
    "prepack": "node --run build",
    "stub": "unbuild --stub",
    "test": "vitest run",
    "dev": "vitest",
    "dev:prepare": "node --run typegen && node --run stub"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vuetifyjs/eslint-config-vuetify.git"
  },
  "keywords": [
    "eslint",
    "config",
    "eslint-config",
    "vue",
    "vuetify"
  ],
  "author": "John Leider <john@vuetifyjs.com>",
  "license": "MIT",
  "peerDependencies": {
    "@vitest/eslint-plugin": "^1.1.42",
    "eslint": "^9.5.0",
    "eslint-plugin-jest": "^28.0.0",
    "eslint-plugin-no-only-tests": "^3.3.0",
    "eslint-plugin-vuejs-accessibility": "^2.0.0"
  },
  "dependencies": {
    "@clack/prompts": "^0.10.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.25.1",
    "@stylistic/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^8.31.0",
    "eslint-config-flat-gitignore": "^2.1.0",
    "eslint-flat-config-utils": "^2.0.1",
    "eslint-plugin-antfu": "^3.1.1",
    "eslint-plugin-import-x": "^4.10.6",
    "eslint-plugin-jsonc": "^2.20.0",
    "eslint-plugin-perfectionist": "^4.12.1",
    "eslint-plugin-pnpm": "^0.3.1",
    "eslint-plugin-regexp": "^2.7.0",
    "eslint-plugin-unicorn": "^58.0.0",
    "eslint-plugin-vue": "^10.0.0",
    "eslint-typegen": "^2.1.0",
    "exsolve": "^1.0.5",
    "globals": "^16.0.0",
    "jsonc-eslint-parser": "^2.4.0",
    "kolorist": "^1.8.0",
    "local-pkg": "^1.1.1",
    "nypm": "^0.6.0",
    "package-manager-detector": "^1.2.0",
    "typescript-eslint": "^8.31.0",
    "vue-eslint-parser": "^10.1.3",
    "yaml-eslint-parser": "^1.3.0"
  },
  "devDependencies": {
    "@types/node": "^22.13.17",
    "eslint-config-vuetify": "file:../eslint-config-vuetify",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "unbuild": "^3.5.0",
    "valibot": "^1.0.0",
    "vitest": "^3.1.2",
    "vue": "^3.5.13"
  },
  "packageManager": "pnpm@8.6.11"
}
